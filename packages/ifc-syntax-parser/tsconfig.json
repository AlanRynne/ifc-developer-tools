{
  "extends": "../../tsconfig.json",
  "compilerOptions": {
    "outDir": "dist",
    "rootDir": "src",
    "skipLibCheck": true,
    "noImplicitReturns": true,
    "strictNullChecks": false,
    "forceConsistentCasingInFileNames": true
  },
  "include": ["src/**/*"],
  "exclude": ["node_modules", "dist, **/*.spec.ts"]
}
